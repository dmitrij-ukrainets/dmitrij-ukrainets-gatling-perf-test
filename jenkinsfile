pipeline {
    agent any
    stages {
        stage("Build Maven") {
            steps {
                bat 'mvn -B clean package'
            }
        }
        stage("Run Gatling") {
            steps {
                bat 'mvn clean gatling:test -Dusers=${users} -Dramp=${ramp}'
            }
            post {
                always {
                    gatlingArchive()
                }
            }
        }
    }
}